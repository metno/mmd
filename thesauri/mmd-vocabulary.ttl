  skos:altLabel "Central Arctic"@en ;
  skos:closeMatch <http://vocab.nerc.ac.uk/mmd/C16/current/17/>, <http://vocab.nerc.ac.uk/mmd/C19/current/9/>, <http://vocab.nerc.ac.uk/mmd/P30/current/arctic_ocean/> ;
