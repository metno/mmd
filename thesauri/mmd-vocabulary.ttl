  skos:prefLabel "environment"@en ;
  skos:altLabel "ENVIRONMENT"@en .

<http://vocab.met.no/mmd/ISO_Topic_Category/extraTerrestrial>
  a skos:Concept ;
  skos:definition ""@en ;
  skos:prefLabel "extraTerrestrial"@en ;
  skos:altLabel "EXTRA TERRESTRIAL"@en .

<http://vocab.met.no/mmd/ISO_Topic_Category/biota>
  a skos:Concept ;
  skos:definition "Flora or fauna in natural environment, for example wildlife, vegetation, biological sciences, ecology, wilderness, sea life, wetlands, habitat, biological resources "@en ;
  skos:prefLabel "biota"@en ;
  skos:altLabel "BIOTA"@en .

<http://vocab.met.no/mmd/ISO_Topic_Category/disaster>
  a skos:Concept ;
  skos:definition ""@en ;
  skos:prefLabel "disaster"@en ;
  skos:altLabel "DISASTER"@en .

<http://vocab.met.no/mmd/ISO_Topic_Category/transportation>
  a skos:Concept ;
  skos:definition "Means and aids for conveying persons or goods, for example roads, airports/airstrips, shipping routes, tunnels nautical charts, vehicle or vessel location, aeronautical charts, railways"@en ;
